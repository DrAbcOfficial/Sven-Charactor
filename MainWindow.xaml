            <Label Content="人物模型安装" Foreground="{Binding Path=DeepColor,Source={x:Static Properties:Settings.Default}}"  FontSize="13" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
